  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@fontsource/roboto": "4.5.8",
    "@mui/icons-material": "5.10.9",
    "@mui/material": "5.10.9",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "13.0.0",
    "@testing-library/user-event": "13.2.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ramda": "0.28.0",
    "react": "18.2.0",
